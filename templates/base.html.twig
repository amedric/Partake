<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <link rel="icon" type="image/png" href="{{ asset('build/images/favicon.png') }}"/>
        <title>
            {% block title %}Partake{% endblock %}
        </title>
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>

    <div class="ptk-baseContainer">
        <div class="ptk-navbarContainer">
            {% include '/_includes/_navbar.html.twig' %}
        </div>
        <div class="ptk-sideContainer">
            <p class="ptk-logo"><a href="{{ path('app_home') }}"></a>PARTAKE</p>
        </div>
        <div class="ptk-blockContainer">
        </div>
    </div>

    <div class="ptk-bodyContainer">
        {% block body %}{% endblock %}
    </div>

    </body>
</html>
